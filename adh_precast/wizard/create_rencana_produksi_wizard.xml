<odoo>
    <data>
    	
       <record id="create_rencana_produksi_wizard" model="ir.ui.view">
            <field name="name">Buat Rencana</field>
            <field name="model">adhimix.create.rencana.produksi.wizard</field>
            <field name="arch" type="xml">
                <form string="Buat Rencana">
                	<group cols="2">	
	                    <group>
	                		<field name="date_from"/>
	                    	<field name="date_to"/>
	                    	<field name="rencana_id" invisble="1"/>
						</group>
						<group>
	                    	<field name="product_id"/>
	                    	<field name="volume"/>
	                    </group> 

					</group>		
                    <footer>
						<button name="create_buat_rencana" string="Buat Rencana" type="object" class="oe_highlight"/>
						                        or
                        <button string="Cancel" class="oe_link" special="cancel" />
                    </footer>
                </form>
            </field>
        </record>

    </data>
</odoo>
